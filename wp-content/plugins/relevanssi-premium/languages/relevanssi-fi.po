msgstr[0] "%1$d taksonomiatermi, yhteensä %2$d / %3$d."
msgstr[1] "%1$d taksonomiatermiä, yhteensä %2$d / %3$d."

#: premium/admin-ajax.php:343
msgid "disabled."
msgstr "pois käytöstä."

#: premium/admin-ajax.php:351
#, php-format
